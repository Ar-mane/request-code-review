{
  "name": "request-code-review-extension",
  "version": "1.0.0",
  "description": "Chrome extension to copy GitHub PR links and commits",
  "type": "module",
  "scripts": {
    "build": "npm run lint && webpack --config webpack.config.cjs --mode=production",
    "build:dev": "npm run lint && webpack --config webpack.config.cjs --mode=development",
    "watch": "webpack --config webpack.config.cjs --mode=development --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "Ar-mane",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ar-mane/request-code-review"
  },
  "keywords": [
    "chrome",
    "extension",
    "github",
    "gitlab",
    "code review"
  ],
  "devDependencies": {
    "@types/chrome": "^0.0.200",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "autoprefixer": "^10.4.20",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.10.0",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss": "^8.4.45",
    "postcss-loader": "^8.1.1",
    "tailwindcss": "^3.4.10",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
